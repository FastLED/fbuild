{
  "name": "ST Nucleo F446RE",
  "board": "nucleo_f446re",
  "mcu": "stm32f446ret6",
  "architecture": "arm-cortex-m4",
  "f_cpu": "180000000L",
  "variant": "STM32F4xx/F446R(C-E)T",
  "product_line": "STM32F446xx",

  "compiler_flags": {
    "common": [
      "-mcpu=cortex-m4",
      "-mthumb",
      "-mfloat-abi=hard",
      "-mfpu=fpv4-sp-d16",
      "-ffunction-sections",
      "-fdata-sections",
      "-Wno-error=unused-function",
      "-Wno-error=unused-variable",
      "-Wno-error=deprecated-declarations",
      "-Wno-unused-parameter",
      "-Wno-sign-compare",
      "-MMD"
    ],
    "c": [
      "-std=gnu17",
      "-Wno-old-style-declaration"
    ],
    "cxx": [
      "-std=gnu++17",
      "-fno-exceptions",
      "-fno-rtti",
      "-fno-threadsafe-statics"
    ]
  },

  "linker_flags": [
    "-mcpu=cortex-m4",
    "-mthumb",
    "-mfloat-abi=hard",
    "-mfpu=fpv4-sp-d16",
    "-Wl,--gc-sections",
    "-Wl,--cref",
    "-Wl,--check-sections",
    "-Wl,--unresolved-symbols=report-all",
    "-Wl,--warn-common",
    "--specs=nano.specs",
    "--specs=nosys.specs",
    "-Wl,--entry=Reset_Handler"
  ],

  "linker_scripts": [
    "ldscript.ld"
  ],

  "linker_libs": [
    "-lgcc",
    "-lstdc++",
    "-lm",
    "-lc"
  ],

  "profiles": {
    "release": {
      "compile_flags": ["-Os", "-flto=auto", "-fno-fat-lto-objects"],
      "link_flags": ["-flto=auto", "-fuse-linker-plugin"]
    },
    "quick": {
      "compile_flags": ["-O1"],
      "link_flags": ["-flto=auto", "-fuse-linker-plugin"]
    }
  },

  "defines": [
    "STM32F4",
    "STM32F446xx",
    "ARDUINO_ARCH_STM32",
    ["STM32_CORE_VERSION_MAJOR", "2"],
    ["STM32_CORE_VERSION_MINOR", "12"],
    "USE_HAL_DRIVER",
    ["F_CPU", "180000000L"]
  ],

  "upload": {
    "maximum_size": 524288,
    "maximum_ram_size": 131072
  }
}
