{
  "name": "Teensy LC",
  "description": "Configuration for Teensy LC board - NXP MKL26Z64 ARM Cortex-M0+ @ 48MHz",
  "board": "teensylc",
  "mcu": "mkl26z64",
  "f_cpu": "48000000L",
  "core": "teensy3",
  "variant": "teensylc",
  "architecture": "arm",

  "compiler_flags": {
    "common": [
      "-mcpu=cortex-m0plus",
      "-mthumb",
      "-Wall",
      "-ffunction-sections",
      "-fdata-sections",
      "-fsingle-precision-constant",
      "-MMD"
    ],
    "c": [
      "-std=gnu11"
    ],
    "cxx": [
      "-std=gnu++14",
      "-fno-exceptions",
      "-fno-rtti",
      "-felide-constructors",
      "-Wno-error=narrowing"
    ]
  },

  "linker_flags": [
    "-mcpu=cortex-m0plus",
    "-mthumb",
    "-Wl,--gc-sections",
    "-Wl,--relax",
    "-Wl,--print-memory-usage",
    "--specs=nano.specs"
  ],

  "linker_scripts": [
    "mkl26z64.ld"
  ],

  "linker_libs": [
    "-lm",
    "-lstdc++",
    "-lc",
    "-lgcc"
  ],

  "profiles": {
    "release": {
      "compile_flags": ["-Os", "-flto=auto", "-fno-fat-lto-objects"],
      "link_flags": ["-Os", "-flto=auto", "-fuse-linker-plugin"]
    },
    "quick": {
      "compile_flags": ["-O1"],
      "link_flags": []
    }
  },

  "defines": [
    "__MKL26Z64__",
    "ARDUINO_ARCH_TEENSY",
    "ARDUINO_TEENSYLC",
    ["ARDUINO", "10819"],
    ["TEENSYDUINO", "159"],
    ["F_BUS", "24000000"],
    "USB_SERIAL",
    "LAYOUT_US_ENGLISH"
  ]
}
