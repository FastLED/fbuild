{
  "name": "NXP MK66FX1M0",
  "description": "Configuration for MK66FX1M0 MCU (Teensy 3.6) - ARM Cortex-M4F @ 180MHz",
  "mcu": "mk66fx1m0",
  "architecture": "arm",

  "compiler_flags": {
    "common": [
      "-mcpu=cortex-m4",
      "-mthumb",
      "-mfloat-abi=hard",
      "-mfpu=fpv4-sp-d16",
      "-Wall",
      "-ffunction-sections",
      "-fdata-sections",
      "-fsingle-precision-constant",
      "-MMD"
    ],
    "c": [
      "-std=gnu11"
    ],
    "cxx": [
      "-std=gnu++14",
      "-fno-exceptions",
      "-fno-rtti",
      "-felide-constructors",
      "-Wno-error=narrowing"
    ]
  },

  "linker_flags": [
    "-mcpu=cortex-m4",
    "-mthumb",
    "-mfloat-abi=hard",
    "-mfpu=fpv4-sp-d16",
    "-Wl,--gc-sections",
    "-Wl,--relax",
    "-Wl,--print-memory-usage",
    "--specs=nano.specs"
  ],

  "linker_scripts": [],

  "linker_libs": [
    "-larm_cortexM4lf_math",
    "-lm",
    "-lstdc++",
    "-lc",
    "-lgcc"
  ],

  "profiles": {
    "release": {
      "compile_flags": ["-Os", "-flto=auto", "-fno-fat-lto-objects"],
      "link_flags": ["-Os", "-flto=auto", "-fuse-linker-plugin"]
    },
    "quick": {
      "compile_flags": ["-O1"],
      "link_flags": []
    }
  },

  "defines": [
    "__MK66FX1M0__",
    "ARDUINO_ARCH_TEENSY",
    ["ARDUINO", "10819"],
    ["TEENSYDUINO", "159"],
    "USB_SERIAL",
    "LAYOUT_US_ENGLISH"
  ]
}
